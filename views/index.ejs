<form action="/upload" method="post" enctype="multipart/form-data">
    <input type="file" name="file" accept="image/*">
    <input type="submit" value="submit">


</form>

<% if(files){%>
    <% files.forEach(function(file){ %>

        <p>
            <% file.filename %>
        </p>
        <img src="image/<%= file.filename%>" width="250" />
        <% }) %>

            <% } else { %>
                <p>No files</p>



                <% } %>